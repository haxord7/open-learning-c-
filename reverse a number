//Reverse a number 
#include<stdio.h>
#include <string.h>
#include<stdlib.h>
int main()
{
char string [1000],temp;
int i,j;
printf("enter a string");
scanf("%s",string);
i=0;
j=strlen(string)-1;
while(i<j) {
temp=string[i];
string[i]=string[j];
string[j]=temp;
i++;
j--;
}
int x;
x=atoi(string);
printf("%d",x);
return 0;
}

